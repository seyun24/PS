#include <bits/stdc++.h>
using namespace std;
int sum;
int main(void) {
    ios::sync_with_stdio(0);
    cin.tie(0);
    string a;
    cin >> a;
    for (auto c : a) {
        if (c - 'C' <= 0) {
            sum += 3;
        }
        else if (c - 'F' <= 0) {
            sum += 4;
        }
        else if (c - 'I' <= 0) {
            sum += 5;
        }
        else if (c - 'L' <= 0) {
            sum += 6;
        }
        else if (c - 'O' <= 0) {
            sum += 7;
        }
        else if (c - 'S' <= 0) {
            sum += 8;
        }
        else if (c - 'V' <= 0) {
            sum += 9;
        }
        else if (c - 'Z' <= 0) {
            sum += 10;
        }

    }
    cout << sum;
}
